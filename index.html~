		<div id="map" style="width: 100%; height: 100%">
			<script type="text/javascript" language = "javascript">
				var map = L.map('map').setView([27.67805, 85.32156], 15);
				var tileSource = 'http://a.tiles.mapbox.com/v3/galligalli.map-3i95v1d3.jsonp';
				wax.tilejson(tileSource, function(tilejson) {
        				map.addLayer(new wax.leaf.connector(tilejson));
    				});
				
			</script>
		</div>
